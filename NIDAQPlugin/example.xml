<?xml version="1.0" standalone="no"?>
<monkeyml version="1.0">
    <io_devices tag="IO Devices">
        <iodevice tag="nidaq" type="nidaq" name="Dev1" analog_input_data_interval="1ms" analog_input_update_interval="3ms" analog_output_data_interval="1ms" assume_multiplexed_adc="1">
            <iochannel tag="nidaq_ai0" type="nidaq_analog_input_voltage" channel_number="0" range_min="-10.0" range_max="10.0" variable="ai0"></iochannel>
            <iochannel tag="nidaq_ai1" type="nidaq_analog_input_voltage" channel_number="1" range_min="-1.0" range_max="1.0" variable="ai1"></iochannel>
            <iochannel tag="nidaq_ao0" type="nidaq_analog_output_voltage_waveform" channel_number="0" range_min="-9.5" range_max="9.5" waveform="sinusoid" period="500ms" offset="0"></iochannel>
            <iochannel tag="nidaq_ao1" type="nidaq_analog_output_voltage_waveform" channel_number="1" range_min="-0.95" range_max="0.95" waveform="sawtooth" period="500ms" offset="0"></iochannel>
        </iodevice>
    </io_devices>
    <variables tag="Variables">
        <variable tag="ai0" scope="global" logging="when_changed" default_value="0" type="float"></variable>
        <variable tag="ai1" scope="global" logging="when_changed" default_value="0" type="float"></variable>
        <variable tag="done" scope="global" logging="when_changed" default_value="0" type="boolean"></variable>
    </variables>
    <sounds tag="Sounds"></sounds>
    <stimuli tag="Stimuli"></stimuli>
    <filters tag="Filters"></filters>
    <optimizers tag="Optimizers"></optimizers>
    <experiment tag="New Experiment" full_name="" description="">
        <protocol tag="New Protocol" nsamples="1" sampling_method="cycles" selection="sequential" interruptible="YES">
            <task_system tag="New Task System" interruptible="YES">
                <task_system_state tag="Begin State System" interruptible="YES">
                    <action_marker _unmoveable="1" tag="Actions"></action_marker>
                    <action type="assignment" tag="Reset done" variable="done" value="0"></action>
                    <action tag="Start IO Device" type="start_device_IO" device="nidaq"></action>
                    <transition_marker _unmoveable="1" tag="Transitions"></transition_marker>
                    <transition type="conditional" tag="If Condition is True, Transition to ..." condition="done" target="Exit State System"></transition>
                </task_system_state>
                <task_system_state tag="Exit State System" interruptible="YES">
                    <action_marker _unmoveable="1" tag="Actions"></action_marker>
                    <action tag="Stop IO Device" type="stop_device_IO" device="nidaq"></action>
                    <transition_marker _unmoveable="1" tag="Transitions"></transition_marker>
                    <transition tag="Return to parent task system" type="yield"></transition>
                </task_system_state>
            </task_system>
        </protocol>
    </experiment>
</monkeyml>