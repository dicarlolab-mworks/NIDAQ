<?xml version="1.0" standalone="no"?>
<monkeyml version="1.0">
    <io_devices tag="IO Devices">
        <iodevice type="nidaq" tag="nidaq" name="Dev1" update_interval="3ms" analog_input_data_interval="1ms" analog_output_data_interval="1ms" assume_multiplexed_adc="YES">
            <iochannel type="nidaq_analog_output_voltage" tag="ao0" channel_number="0" range_min="range_min" range_max="range_max" voltage="output"></iochannel>
            <iochannel type="nidaq_analog_input_voltage" tag="ai0" channel_number="0" range_min="range_min" range_max="range_max" voltage="input"></iochannel>
        </iodevice>
    </io_devices>
    <variables tag="Variables">
        <variable tag="range_min" scope="global" logging="when_changed" default_value="-9.5" type="float"></variable>
        <variable tag="range_max" scope="global" logging="when_changed" default_value="9.5" type="float"></variable>
        <variable tag="output" scope="global" logging="when_changed" default_value="0" type="integer"></variable>
        <variable tag="input" scope="global" logging="when_changed" default_value="0" type="integer"></variable>
    </variables>
    <sounds tag="Sounds"></sounds>
    <stimuli tag="Stimuli">
        <stimulus type="frame_list" tag="frame_list" stimulus_group="frames" ending="" ended="" loop="YES" repeats="0" autoplay="YES"></stimulus>
        <stimulus_group tag="frames">
            <stimulus type="rectangle" tag="box" color="1,1,1" x_size="1" y_size="1" x_position="input" y_position="0" rotation="0" alpha_multiplier="1"></stimulus>
        </stimulus_group>
    </stimuli>
    <filters tag="Filters"></filters>
    <optimizers tag="Optimizers"></optimizers>
    <experiment tag="New Experiment" full_name="" description="">
        <protocol tag="New Protocol" nsamples="1" sampling_method="cycles" selection="sequential" interruptible="YES">
            <action type="assignment" tag="Reset output" variable="output" value="0"></action>
            <action tag="Start nidaq" type="start_device_IO" device="nidaq"></action>
            <action tag="Live queue frame list" type="live_queue_stimulus" stimulus="frame_list"></action>
            <action tag="Update display" type="update_stimulus_display"></action>
            <list tag="New List" nsamples="5" sampling_method="cycles" selection="sequential" interruptible="YES">
                <action type="wait" tag="Wait" duration="2" duration_units="s"></action>
                <action type="assert" tag="Assert" condition="abs(input - output) &lt; 0.005" message="" stop_on_failure="0"></action>
                <action type="assignment" tag="Set output" variable="output" value="rand(range_min, range_max)"></action>
            </list>
            <action type="wait" tag="Wait" duration="2" duration_units="s"></action>
            <action tag="Dequeue frame list" type="dequeue_stimulus" stimulus="frame_list"></action>
            <action tag="Update display" type="update_stimulus_display"></action>
            <action tag="Stop nidaq" type="stop_device_IO" device="nidaq"></action>
        </protocol>
    </experiment>
</monkeyml>